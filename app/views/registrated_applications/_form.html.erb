<%= form_for @app do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>
  <%= f.button "create", class: "btn btn-primary", data: {disable_with: "<i class='fa fa-spinner'></i>Creating......"} %>
<% end %>